#include <bits/stdc++.h>
using namespace std;

int main() {
	int t;
	cin >> t;
	while(t--)
	{
	    int n;
	    cin >> n;
	    int c=0;
	    vector<int>v;
	    for(int i=0;i<n;i++)
	    {
	        int x;
	        cin >> x;
	        v.push_back(x);
	    }
	    for(int i=0;i<n;i++){
	        if(v[i]>=1000){
	            c++;
	        }
	    }
	    cout << c << endl;
	}// your code goes here
	return 0;
}
